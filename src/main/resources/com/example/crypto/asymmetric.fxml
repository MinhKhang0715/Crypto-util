<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="613.0" prefWidth="779.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.crypto.Asymmetric">
   <TextArea fx:id="txt_plainText" layoutX="50.0" layoutY="100.0" prefHeight="124.0" prefWidth="206.0"/>
   <Label layoutX="50.0" layoutY="84.0" text="Plain text"/>
   <TextArea fx:id="txt_decryptedText" editable="false" layoutX="50.0" layoutY="292.0" prefHeight="124.0"
             prefWidth="206.0"/>
   <Label layoutX="50.0" layoutY="276.0" text="Decrypted text"/>
   <TextArea fx:id="txt_encryptedText" editable="false" layoutX="450.0" layoutY="100.0" prefHeight="323.0"
             prefWidth="206.0"/>
   <Label layoutX="450.0" layoutY="84.0" text="Encrypted text"/>
   <Button layoutX="314.0" layoutY="150.0" mnemonicParsing="false" onAction="#onEncryptButtonClick" text="Encrypt"/>
   <Button layoutX="317.0" layoutY="342.0" mnemonicParsing="false" onAction="#onDecryptButtonClick" text="Decrypt"/>
   <TextArea fx:id="txt_publicKey" editable="false" layoutX="50.0" layoutY="463.0" prefHeight="92.0" prefWidth="276.0"/>
   <Label layoutX="50.0" layoutY="447.0" text="Public key"/>
   <TextArea fx:id="txt_privateKey" editable="false" layoutX="450.0" layoutY="463.0" prefHeight="92.0"
             prefWidth="276.0"/>
   <Label layoutX="450.0" layoutY="447.0" text="Private key"/>
</AnchorPane>
